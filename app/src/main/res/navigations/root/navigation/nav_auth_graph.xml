<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    app:startDestination="@id/mainAuthFragment">
    <fragment
        android:id="@+id/mainAuthFragment"
        android:name="com.mihailchistousov.navgraphs.screens.auth.main.MainAuthFragment"
        android:label="MainAuthFragment"
        tools:layout="@layout/main">
        <action
            android:id="@+id/to_main_2"
            app:destination="@id/main2Fragment" />
    </fragment>
    <fragment
        android:id="@+id/main2Fragment"
        android:name="com.mihailchistousov.navgraphs.screens.auth.main2.Main2Fragment"
        android:label="Main2Fragment"
        tools:layout="@layout/main2"/>

    <action
        android:id="@+id/logOut"
        app:destination="@id/choiceFragment"
        app:popUpToInclusive="true"
        app:popUpTo="@id/main">
        <argument
            android:name="isNeedAnim"
            app:argType="boolean"
            android:defaultValue="false" />
    </action>
</navigation>
